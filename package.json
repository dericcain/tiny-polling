{
  "name": "tiny-polling",
  "version": "1.0.1",
  "repository": "git@github.com:dericgw/tiny-polling.git",
  "author": "Deric Cain <deric.cain@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/tiny-polling.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdx build",
    "coveralls": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tsdx lint ./src/index.ts",
    "preversion": "npm run build",
    "release": "np",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint ./src/index.ts"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "coveralls": "^3.0.6",
    "husky": "^3.0.5",
    "np": "^5.0.3",
    "tsdx": "^0.9.1",
    "tslib": "^1.10.0",
    "typescript": "^3.6.2"
  }
}
